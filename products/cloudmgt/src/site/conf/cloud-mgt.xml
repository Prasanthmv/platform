<?xml version="1.0" encoding="ISO-8859-1"?>
<cloudmgt>
	<subscriptions>
		<type display_name="App Cloud">app_cloud</type>
		<type display_name="Integration Cloud">integration_cloud</type>
		<type display_name="API Cloud">api_cloud</type>
         </subscriptions>
     <ServerUrls>
        <AppFactory>https://localhost:9443/services/</AppFactory>
        <BPS>https://localhost:9543/services/</BPS>
        <Greg>https://localhost:9443/services/</Greg>
    </ServerUrls>
    <SSORelyingParty>
        <Name>appserver</Name>
                <DashboardName>WSO2 AppFactory Dashboard</DashboardName>
        <IdentityProviderURL>https://localhost:9443/samlsso</IdentityProviderURL>
        <KeyStorePassword>wso2carbon</KeyStorePassword>
        <IdentityKeyAlias>wso2carbon</IdentityKeyAlias>
        <KeyStoreName>${carbon.home}/repository/resources/security/wso2carbon.jks</KeyStoreName>
        <CookieRefreshInterval>300000</CookieRefreshInterval>
         <AdminUserName>admin</AdminUserName>
         <AdminPassword>admin</AdminPassword>
    </SSORelyingParty>


</cloudmgt>
