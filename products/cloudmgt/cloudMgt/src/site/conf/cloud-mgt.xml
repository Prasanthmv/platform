<?xml version="1.0" encoding="ISO-8859-1"?>
<cloudmgt>
	<subscriptions>
		<type display_name="APP Cloud">app_cloud</type>
		<type display_name="Integration Cloud">integration_cloud</type>
		<type display_name="API Cloud">api_cloud</type>
         </subscriptions>
	<dbConfig name="cloud_mgt">
		<url>jdbc:mysql://localhost:3306/cloud_mgt</url>
		<userName>root</userName>
		<password>root</password>
		<driverName>com.mysql.jdbc.Driver</driverName>
	</dbConfig>
     <ServerUrls>
        <AppFactory>https://milestones.appfactory.wso2.com:9443/services/</AppFactory>
        <BPS>https://milestones.appfactory.wso2.com:9543/services/</BPS>
        <Greg>https://milestones.appfactory.wso2.com:9443/services/</Greg>
    </ServerUrls>
    <SSORelyingParty>
        <Name>appfactory</Name>
                <DashboardName>WSO2 AppFactory Dashboard</DashboardName>
        <IdentityProviderURL>https://localhost:9443/samlsso</IdentityProviderURL>
        <KeyStorePassword>wso2carbon</KeyStorePassword>
        <IdentityKeyAlias>wso2carbon</IdentityKeyAlias>
        <KeyStoreName>${carbon.home}/repository/resources/security/wso2carbon.jks</KeyStoreName>
    </SSORelyingParty>


</cloudmgt>
