<template xmlns="http://ws.apache.org/ns/synapse" name="getVersionsOfProject">
	<parameter name="projectKey"
		description="a String containing of the unique key for a project" />

	<sequence>
		 <property name="uri.var.projectKey" expression="$func:projectKey" />
		 <property name="messageType" value="application/json" scope="axis2" />
		<send continuation="true">
			<endpoint>
				<http method="get" uri-template="{uri.var.jira.url}/rest/api/2/project/{uri.var.projectKey}/versions" />
			</endpoint>
		</send>
	</sequence>
</template>
