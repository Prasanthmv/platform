<?xml version="1.0" encoding="UTF-8"?>
<template name="configure" xmlns="http://ws.apache.org/ns/synapse">
    <!--This template should be called prior to other Twilio templates. Once this has been added to
         the flow, any number of templates can access the values.
     -->
    <!--Required.-->
    <parameter name="accountSid"/>
    <parameter name="authToken"/>
    <sequence>
        <property expression="$func:accountSid" name="TwilioAccountSid"/>
        <property expression="$func:authToken" name="TwilioAuthToken"/>
    </sequence>
</template>
